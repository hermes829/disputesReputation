@ARTICLE{huckfeldt:sprague:1987,
  author = {Huckfeldt, Robert and Sprague, John},
  title = {Networks in Context: The Social Flow of Political Information},
  journal = {The American Political Science Review},
  year = {1987},
  volume = {81},
  pages = {1197-1216},
  number = {4},
  month = {December},
  abstract = {We examine the effects of individual political preferences and the
	distribution of such preferences on the social transmission of political
	information. Our data base combines a 1984 election survey of citizens
	in South Bend, Indiana with a subsequent survey of people with whom
	these citizens discuss politics. Several findings emerge from the
	effort. First, individuals do purposefully construct informational
	networks corresponding to their own political preferences, and they
	also selectively misperceive socially supplied political information.
