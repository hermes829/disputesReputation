@Article{garnham:1976,
  Title                    = {{Dyadic International War 1816-1965: The Role of Power Parity and Geographical Proximity}},
  Author                   = {Garnham, David},
  Journal                  = {The Western Political Quarterly},
  Year                     = {1976},
  Number                   = {2},
  Pages                    = {pp. 231-242},
  Volume                   = {29},

  Abstract                 = {This article tests the hypotheses that interstate war is more probable (a) between nation-states of approximately equal national power (power parity) and (b) between geographically proximate nation-states. The data base consists of thirty dyadic interstate wars during the period 1816-1965. The analysis employs four indicators of national power: geographical area, population, fuel consumption, and steel production. Hypotheses are tested using the Kolmogorov-Smirnov one-sample test. The proximity hypothesis is confirmed (p&lt;.01). The power parity hypothesis is partially confirmed; except for the steel production analysis, all results are in the predicted direction. The population hypothesis is confirmed (p&lt;.025), and there is considerable support for the geographical area hypothesis (p&lt;.10). The findings justify further dyadic and nondyadic analyses and an effort to develop a power parity theory of interstate war.},
  Copyright                = {Copyright {\copyright} 1976 University of Utah},
  Date-added               = {2013-05-31 21:52:09 +0000},
  Date-modified            = {2013-05-31 21:52:10 +0000},
  ISSN                     = {00434078},
  Jstor_articletype        = {research-article},
  Jstor_formatteddate      = {Jun., 1976},
  Language                 = {English},
  Publisher                = {University of Utah on behalf of the Western Political Science Association}
}

