@Article{lewis-beck:rice:1984,
  Title                    = {Forecasting Presidential Elections: A Comparison of Naive Models},
  Author                   = {Lewis-Beck, Michael S. and Rice, Tom W.},
  Journal                  = {Political Behavior},
  Year                     = {1984},
  Number                   = {1},
  Pages                    = {9--21},
  Volume                   = {6},

  Abstract                 = {Our primary aim is to forecast, rather than explain, presidential election results, using aggregate time series data from the post-World War II period. More particularly, we seek prediction of the presidential winner well before the election actually occurs. After comparing the performance of several naive bivariate models based on economic performance, international involvement, political experience, and presidential popularity, we go on to formulate a multivariate model. This economy-popularity regression model rather accurately forecasts the winner 6 months in advance of the election, by employing spring measures of presidential popularity and the growth rate in real GNP per capita. Furthermore, the model's performance, both ex post facto and prior to the election, compares favorably with the Gallup final preelection poll taken only a few days before the election.},
  Copyright                = {Copyright 1984 Springer},
  Date-added               = {2010-07-01 16:16:06 -0700},
  Date-modified            = {2013-05-24 15:41:19 -0500},
  ISSN                     = {01909320},
  Jstor_articletype        = {primary_article},
  Jstor_formatteddate      = {1984},
  Publisher                = {Springer}
}

@Article{lewis-beck:rice:1984a,
  Title                    = {Forecasting U. S. House Elections},
  Author                   = {Lewis-Beck, Michael S. and Rice, Tom W.},
  Journal                  = {Legislative Studies Quarterly},
  Year                     = {1984},
  Number                   = {3},
  Pages                    = {475--486},
  Volume                   = {9},

  Abstract                 = {We offer a model of incumbent party seat change in House elections which provides true forecasts-it "calls" the seat change well in advance of the election itself. The model predicts seat change as a function of both quarterly real GNP per capita growth and presidential popularity measured six months prior to the election, plus the time of the election (midterm or not). This model is more attractive than the earlier, strictly midterm, models propounded by Tufte and Hibbs. It is clearly superior in forecasting the 1982 results. In general, the model's forecasts for postwar House races are very close to the actual result, off only five or six seats at the median, regardless of whether the forecast is made ex post or ex ante. As a final exercise, we apply the model to 1984, in order to forecast the prospective Republican House seat change.},
  Copyright                = {Copyright 1984 Comparative Legislative Research Center},
  Date-added               = {2010-07-01 16:16:06 -0700},
  Date-modified            = {2013-05-24 15:41:19 -0500},
  ISSN                     = {03629805},
  Jstor_articletype        = {primary_article},
  Jstor_formatteddate      = {Aug., 1984},
  Publisher                = {Comparative Legislative Research Center}
}

